{
    "name": "big-hair-energy/laravel-preview",
    "version": "v1.0.0",
    "description": "Laravel middleware for website access during a preview mode.",
    "license": "MIT",
    "authors": [
        {
            "name": "Big Hair Energy",
            "email": "engineering@bighairenergy.com"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2"
    },
    "autoload": {
        "psr-4": {
            "BigHairEnergy\\Preview\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BigHairEnergy\\Preview\\Tests\\": "tests/"
        }
    }
}
